<?xml version="1.0" standalone="yes"?>
<xs:schema id="dsPatients" targetNamespace="http://www.tempuri.org/dsPatients.xsd" xmlns:mstns="http://www.tempuri.org/dsPatients.xsd" xmlns="http://www.tempuri.org/dsPatients.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dsPatients" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Patients">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Address" type="xs:string" minOccurs="0" />
              <xs:element name="City" type="xs:string" minOccurs="0" />
              <xs:element name="Comments" type="xs:string" minOccurs="0" />
              <xs:element name="CustomerID" type="xs:string" minOccurs="0" />
              <xs:element name="FirstName" type="xs:string" minOccurs="0" />
              <xs:element name="LastName" type="xs:string" minOccurs="0" />
              <xs:element name="OrgID" type="xs:string" minOccurs="0" />
              <xs:element name="PatientFax" type="xs:string" minOccurs="0" />
              <xs:element name="PatientID" msdata:AutoIncrement="true" type="xs:int" />
              <xs:element name="PatientPhone" type="xs:string" minOccurs="0" />
              <xs:element name="Physican" type="xs:string" minOccurs="0" />
              <xs:element name="ReportsNumSaved" type="xs:string" minOccurs="0" />
              <xs:element name="StartDate" type="xs:dateTime" minOccurs="0" />
              <xs:element name="State" type="xs:string" minOccurs="0" />
              <xs:element name="Zip" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:Patients" />
      <xs:field xpath="mstns:PatientID" />
    </xs:unique>
  </xs:element>
</xs:schema>